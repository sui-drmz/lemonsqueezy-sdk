{
    "name": "artbees/lemonsqueezy-sdk",
    "version": "0.0.1",
    "description": "PHP SDK for the Lemonsqueezy API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Farshid Rahimi",
            "email": "suidrmz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^3.0",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Artbees\\Lemonsqueezy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artbees\\Lemonsqueezy\\Tests\\": "tests/"
        }
    },
    "scripts": {},
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable"
}